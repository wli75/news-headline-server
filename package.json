{
  "name": "news-headline-server",
  "version": "1.0.0",
  "description": "A simple server that fetches headlines from google news",
  "main": "build/index.js",
  "scripts": {
    "start": "node build/index.js",
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc -p tsconfig.build.json",
    "lint": "eslint '*/**/*.ts' --fix",
    "test": "jest --coverage --verbose"
  },
  "keywords": [
    "google-news"
  ],
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "he": "^1.2.0",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.4.3",
    "morgan": "^1.9.1",
    "puppeteer": "^1.20.0",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/he": "^1.1.0",
    "@types/jest": "^24.0.18",
    "@types/morgan": "^1.7.37",
    "@types/puppeteer": "^1.20.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  }
}
